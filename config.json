{
  "name": "bc-skeleton",
  "version": "1.0",
  "css_compiler": "scss",
  "autoprefixer_cascade": true,
  "autoprefixer_browsers": ["last 3 versions"],
  "jspm": {
    "dev": {
      "dep_location": "assets/js/dependency-bundle.js",
      "bootstrap": "js/**/* - [js/**/*]"
    },
    "bootstrap": "js/app",
    "bundle_location": "assets/js/bundle.js",
    "jspm_packages_path": "assets/jspm_packages"
  },
  "images": {
    "logo": {
      "width": 250,
      "height": 100
    },
    "large": {
      "width": 1280,
      "height": 1280
    }
  },
  "variations": [
    {
      "name": "Default",
      "settings": {

      }
    }
  ]
}
